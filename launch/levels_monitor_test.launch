<?xml version="1.0"?>
<!-- Example of a launcher for a vumeter node.
     For mor information try to run the node using rosrun
     $ rosrun rospypavumeter vumeter -h -->

<launch>

  <arg name="robot"/>

    <group ns="$(arg robot)">

    	<include file="$(find rospypavumeter)/launch/levels_monitor.launch"/>

    </group>

</launch>
